{{> enTete}} {{> menu}}
<h1>Liste des personnes enregistrées</h1>
<table>
    <tr>
	<th>Numéro</th>
	<th>Nom</th>
	<th>Prénom</th>
    </tr>
    {{#each listePersonne}}
    <tr>
	<td><a href="/listerPersonne/{{this.per_num}}">{{this.per_num}}</a></td>
	<td>{{this.per_nom}}</td>
	<td>{{this.per_prenom}}</td>
    </tr>
    {{/each}}
</table>
